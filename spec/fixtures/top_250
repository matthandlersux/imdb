        <script type="text/javascript">
                jQuery(
                             function() {
            // set the initial state of the lister
            $(".lister").lister({
               initialParams : {
                   sort : 'ir:descending'
               },
               viewMoreOption : 'NONE'
            });
            
            // init rating & watchlist
            $("a.wlb_watchlist_lite").wlb_lite();
            $("table.chart").watchlistRibbon("div.wlb_ribbon");
            $('.seen-widget')
                // Init sorting data
                .on('seen-init', function() {
                    $('.seen-widget').trigger('seen-updated');
                })
                // Login when unregistered user clicks on seen widget.
                .on('seen-not-logged-in', function() {
                    $('#nblogin').click();
                })
                // Update sorting data when rating changes
                .on('seen-updated', function() {
                    var $w = $(this);
                    var rating = $w.data('rating');
                    if (rating == null) {
                        rating = $w.hasClass('seen') ? 0.5 : 0;
                    }
                    $w.closest('[name=ur]').data('value', rating);
                });
        }

                );
        </script>

